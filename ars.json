{
  "version": "0.1.0",
  "analyses": [
    {
      "analysis_id": "DM_AGE_SUMMARY",
      "dataset": "ADSL",
      "population": {
        "filter": "SAFFL == \"Y\""
      },
      "variable": {
        "name": "AGE",
        "label": "Age (years)"
      },
      "group_by": ["ARM"],
      "statistics": ["N", "MEAN", "SD", "MEDIAN", "Q1", "Q3", "MIN", "MAX"],
      "presentation": {
        "title": "Summary of Age by Treatment Arm",
        "rounding": {
          "decimals": 1
        }
      }
    }
  ]
}
